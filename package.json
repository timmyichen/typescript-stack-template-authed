{
  "name": "ts-template-authed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "prod:server": "NODE_ENV=production ts-node --files --transpile-only ./server/index.ts",
    "dev:server": "ts-node --files --project ./tsconfig.server.json --transpile-only ./server/index.ts",
    "start": "npm run prod:server",
    "dev": "./node_modules/nodemon/bin/nodemon.js --watch 'server/**/*.ts' --exec 'npm run dev:server'",
    "nodemon": "./node_modules/nodemon/bin/nodemon.js",
    "next:build": "next build",
    "prod:build": "NODE_OPTIONS=--max_old_space_size=1024 next build",
    "lint:ts": "tslint -c tslint.json '**.ts*'",
    "pretty": "prettier --write client/**/*.ts* server/**/*.ts",
    "precommit": "lint-staged",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "gql:gen": "./node_modules/.bin/graphql-codegen --config ./generated-gql/codegen.yml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^4.1.2",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.9",
    "apollo-upload-client": "^11.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "camelize": "^1.0.0",
    "classnames": "^2.2.6",
    "connect-session-sequelize": "^6.0.0",
    "dataloader": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-router-async": "^0.3.0",
    "express-session": "^1.17.0",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-upload": "^8.1.0",
    "hijackresponse": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "next": "^9.1.3",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "semantic-ui-react": "^0.88.1",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/fragment-matcher": "^1.8.3",
    "@graphql-codegen/introspection": "^1.8.3",
    "@graphql-codegen/typescript": "^1.8.3",
    "@graphql-codegen/typescript-operations": "^1.8.3",
    "@graphql-codegen/typescript-react-apollo": "^1.8.3",
