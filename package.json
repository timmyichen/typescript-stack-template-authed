    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run pretty",
      "git add"
